 = R * 2 * PI; \n\nreturn (D * y)\/x;\n\n}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n\nSLONG numberOfDays(SLONG cycleTime[],SLONG planets)\n{\nSLONG day = 0;\nSLONG angles [planets]; \n\ndo{\n\tday++;\n\tupdateAngles(angles,cycleTime,planets,day);\n\t\n\t}while(!alignment(angles,planets));\n\nreturn day;\n}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\nvoid updateAngles(SLONG angles[],SLONG cycleTime[],\n\t\t\t\tSLONG planets,SLONG day)\n{\nfor(int i = 0 ;i&LT;planets;i++)\n\tangles[i] = calcAngle(cycleTime[i],day);\n\n}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\nSLONG calcAngle(SLONG cycleTime,SLONG day)\n{\nif (day % cycleTime == 0)\n\treturn 0;\nreturn (day * 360)\/cycleTime;\n}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\nbool alignment(SLONG angles[],SLONG planets)\n{\nfor(int i=0;i&LT;planets;i++)\n\tif(angles[i] != angles[0])\n\t\treturn false;\n\nreturn true;\n}\n","answer_submit":1,"evaluation":300,"result":"332640","lang":"C++","PHPDBID":31,"answerTimes":1},{"user_name":"mr.coder","algorithm_id":5,"start_time":1380292171,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":32,"answerTimes":0},{"user_name":"zetrix","algorithm_id":5,"start_time":1380298103,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":33,"answerTimes":0},{"user_name":"ihope","algorithm_id":5,"start_time":1380299879,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":34,"answerTimes":0},{"user_name":"\u0631\u0641\u064a\u0642 \u0627\u0644\u0627\u0645\u0644","algorithm_id":5,"start_time":1380307471,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":35,"answerTimes":0},{"user_name":"fmgret12","algorithm_id":5,"