\t\t\t\tprintf(\"%lld\\n\",\n\t\t\t\t\t\t\t\t\tO(\n\t\t\t\t\t\t\t\t\tO(\n\t\t\t\t\t\t\t\t\tO(\n\t\t\t\t\t\t\t\t\tO(\n\t\t\t\t\t\t\t\t\tO(\n\t\t\t\t\t\t\t\t\tO(b[0],b[1],op[i]),b[2],op[j]),b[3],op[k]),b[4],op[l]),b[5],op[m]),b[6],op[n])\n\t\t\t\t\t\t\t\t\t);\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\/\/printf(\"%lld\\n\",buf[6]);\n\t\t\t\t\t\t}\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n\treturn 0;\n}","answer_submit":1,"evaluation":300,"result":"5513880","lang":"C++","PHPDBID":47,"answerTimes":1},{"user_name":"ihope","algorithm_id":6,"start_time":1380878938,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":48,"answerTimes":0},{"user_name":"zaraki_kenpache","algorithm_id":6,"start_time":1380898815,"end_time":1380905935,"answer":"\n\/\/the output is :\n\/\/8541+845-7415*415\/15*852-85145=46375267\n\/\/Press any key to continue . . .\n\n\/\/so the result is( + - * \/* -)\n\/\/and you can try this ((((((8541+845)-7415)*415)\/15)*852)-85145)=46375267 from lift to right as you say\nusing System;\nusing System.Collections.Generic;\nusing System.Linq;\nusing System.Text;\n\nnamespace ConsoleApplication5\n{\n    class Program\n    {\n        static void Main(string[] args)\n        {\n            Int64[] number = { 8541, 845, 7415, 415, 15, 852, 85145 };\/\/there are no zero \n            Int64 FainalResult = 46375267;\n            Int64 result = number[0];\/\/=8541 because there are no zero\n            for (Int64 i = 1; i &LT; 5; i++)\n            {\n                for (Int64 j = 1; j &LT; 5; j++)\n                {\n                    for (Int64 k = 1; k &LT; 5; k++)\n                    {\n                        for (Int64 l = 1; l &LT; 5; l++)\n                        {\n                            for (Int64 m = 1; m &LT; 5; m++)\n                            {\n                                for (Int64 n = 1; n &LT; 5; n++)\n                                {\n                                    char[] AllOperators = { '*', '*', '*', '*', '*', '*' };\n                                    Int64[] operator2 = { i, j, k, l, m, n };\n                                    for (Int64 r = 0; r &LT; 6; r++)\n                                    {\n\n\n                                        switch (operator2[r])\n                                        {\n                                            case 1:\n                                                { result = result * number[r + 1]; AllOperators[r] = '*'; }\n                                                break;\n                                            case 2:\n                                                { result = result \/ number[r + 1]; AllOperators[r] = '\/'; }\n                                                break;\n                                            case 3:\n                                                { result = result + number[r + 1]; AllOperators[r] = '+'; }\n                                                break;\n                                            case 4:\n                                                { result = result - number[r + 1]; AllOperators[r] = '-'; }\n                                                break;\n                                        }\n                                    }\n\n                                    if (result == FainalResult)\/\/if rusult=46375267\n                                    {\n                                        Console.Write(number[0].ToString());\n                                        for (int x = 0; x &LT; 6; x++)\n                                        {\n                                            Console.Write(AllOperators[x].ToString() + number[x + 1].ToString());\n                                        }\n                                        Console.WriteLine(\"=\" + result.ToString());\/\/8541+845-7415*415\/15*852-85145=46375267\n                                    }\n                                    result = number[0];\n                                }\n\n                            }\n                        }\n\n                    }\n                }\n\n            }\n\n        }\n    }\n}\n","answer_submit":1,"evaluation":300,"result":"8541 + 845 - 7415 * 415 \/ 15 * 852 - 85145","lang":"C#","PHPDBID":49,"answerTimes":1},{"user_name":"dracola168","algorithm_id":6,"start_time":1380920894,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":50,"answerTimes":0},{"user_name":"abdou001","algorithm_id":6,"start_time":1380970654,"end_time":1380995393,"answer":"\/\/5513880\n\/\/c++\n\n#include &LT;iostream&GT;\n\nusing namespace std;\n\ntypedef long long LLONG; \n\nLLONG chooseOperation(LLONG a,LLONG b,char op);\nLLONG add(LLONG a,LLONG b);\nLLONG sub(LLONG a,LLONG b);\nLLONG multp(LLONG a,LLONG b);\nLLONG div(LLONG a,LLONG b);\nbool test(LLONG numbers[],char *opr,LLONG result);\nLLONG calc(LLONG numbers[],char *op,LLONG result,LLONG numbers2[]);\nvoid switchOp(char *opr,int pos);\n\nint main()\n{\n\nchar finalSet[6];\nchar opr[7]=\"++++++\";\n\nLLONG numbers[]={8541,845,7415,415,15,852,85145};\nLLONG numbers2[]={8961,412,665,4215,412,62,9514};\n\nLLONG result = 46375267;\n\ncout &LT;&LT; calc(numbers,opr,result,numbers2) &LT;&LT;endl;\n\nreturn 0;\n}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\nLLONG chooseOperation(LLONG a,LLONG b,char op)\n{\nswitch(op)\n\t{\n\tcase '+' : \n\t\treturn add(a,b);\n\tcase '-' :\n\t\treturn sub(a,b);\n\tcase '*' :\n\t\treturn multp(a,b);\n\tdefault:\n\t\treturn div(a,b);\n\t}\n}\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\nLLONG add(LLONG a,LLONG b)\n\t{\n\treturn a+b;\n\t}\nLLONG sub(LLONG a,LLONG b)\n\t{\n\treturn a-b;\n\t}\nLLONG multp(LLONG a,LLONG b)\n\t{\n\treturn a*b;\n\t}\n\t\nLLONG div(LLONG a,LLONG b)\n\t{\n\treturn a\/b;\n\t}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\n\nLLONG calc(LLONG numbers[],char *opr,LLONG result,LLONG numbers2[])\n{\nLLONG z = result;\nLLONG nCopy[7];\n\nfor(int i=0;i&LT;7;i++)\n\tnCopy[i] = numbers[i];\n\nchar op[]=\"+-*\/\";\nint pos,oprP;\n\n\npos = 0;\t\nwhile(!test(numbers,opr,result))\n\t{\n\tswitchOp(opr,pos);\n\t}\n\t\nLLONG s = numbers2[0];\nfor(int i=1;i&LT;7;i++)\n\ts = chooseOperation(s,numbers2[i],opr[i-1]);\n\t\n\t\nreturn s;\n\n\t\n}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\nbool test(LLONG numbers[],char *opr,LLONG result)\n{\nLLONG s = numbers[0];\nfor(int i=1;i&LT;7;i++)\n\t{\n\tif(opr[i]=='\/' && numbers[i]==0)\n\t\treturn false;\n\ts = chooseOperation(s,numbers[i],opr[i-1]);\n\t}\n\t\n\nreturn s==result;\n}\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\nvoid switchOp(char *opr,int pos)\n{\n\t\n\tif(opr[pos]=='+')\n\t\topr[pos]='-';\n\telse if(opr[pos]=='-')\n\t\topr[pos]='*';\n\telse if(opr[pos]=='*')\n\t\topr[pos]='\/';\n\telse if(opr[pos]=='\/')\n\t\t{\n\t\tswitchOp(opr,++pos);\n\t\tif(opr&GT;0)\n\t\tfor(int i=0;i&LT;pos;i++)\n\t\t\topr[i]='+';\n\t\t}\n\t\n}\n","answer_submit":1,"evaluation":300,"result":"5513880","lang":"C++","PHPDBID":51,"answerTimes":1},{"user_name":"SharpSword","algorithm_id":6,"start_time":1381003452,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":52,"answerTimes":0},{"user_name":"AGASHE","algorithm_id":6,"start_time":1381061510,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":53,"answerTimes":0},{"user_name":"M@gici@n","algorithm_id":6,"start_time":1381063389,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":54,"answerTimes":0},{"user_name":"Malak1","algorithm_id":6,"start_time":1381076493,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":55,"answerTimes":0},{"user_name":"zetrix","algorithm_id":6,"start_time":1381079056,"end_time":1381163858,"answer":"#region Week 5\n        \/\/-------------------------------------------------\n        \/\/Full Name : Abdelalim ZERKANI\n        \/\/Language : C#\n        \/\/Solution : 5513936.58252427 with the set of operators (+ - * \/ * -) in this order from left to right.\n        \/\/-------------------------------------------------\n        ArrayList UsedStrings;\n\n        public void main()\n        {\n            \/\/Week 5\n            string combination = getRightCombination().ToString();\n            MessageBox.Show(Calculate(combination).ToString());\n        }\n\n        public string getRightCombination()\n        {\n\n            double[] tab = new double[] { 8541, 845, 7415, 415, 15, 852, 85145 };\n            double result = 46375267;\n\n            \/\/Fill the list with all possible combination of the operators (which is 4 power 6 = 4096)\n            UsedStrings = FillStringList();\n\n            \/\/Calculate thr temporary result of each combination and cmpare it to the result above\n            double tempResult = 0;\n            for (int count=0; count &LT; UsedStrings.Count;count++ )\n            {\n                tempResult = tab[0];\n\n                \/\/Calculate without taking priority in consideration\n                for (int index = 1; index &LT; tab.Length; index++)\n                {\n                    switch (((string)UsedStrings[count])[index-1])\n                    {\n                        case '+':\/\/plus\n                            tempResult += tab[index];\n                            break;\n                        case '-':\/\/Minus\n                            tempResult -= tab[index];\n                            break;\n                        case '*':\/\/Mutiply\n                            tempResult *= tab[index];\n                            break;\n                        case '\/':\/\/Divide\n                            try\n                            {\n                                tempResult \/= tab[index];\n                            }\n                            catch (Exception) { }\n                            break;\n                    }\n                }\n                if (tempResult == result)\n                {\n                    \/\/return the right combination\n                    return (string)UsedStrings[count];\n                }\n            }\n            return \"FALSE\";\n        }\n\n        public double Calculate(string strOperators)\n        {\n            double[] tab = new double[] { 8961, 412, 665, 4215, 412, 62, 9514 };\n            double result = tab[0];\n            \/\/Calculate without taking priority in consideration\n            for (int index = 1; index &LT; tab.Length; index++)\n            {\n                switch (strOperators[index - 1])\n                {\n                    case '+':\/\/plus\n                        result += tab[index];\n                        break;\n                    case '-':\/\/Minus\n                        result -= tab[index];\n                        break;\n                    case '*':\/\/Mutiply\n                        result *= tab[index];\n                        break;\n                    case '\/':\/\/Divide\n                        try\n                        {\n                            result \/= tab[index];\n                        }\n                        catch (Exception) { }\n                        break;\n                }\n            }\n\n            return result;\n        }\n\n        public ArrayList FillStringList()\n        {\n            string str = \"\";\n            ArrayList array = new ArrayList();\n            for (int i = 1; i &LT;= 4; i++)\n                for (int j = 1; j &LT;= 4; j++)\n                    for (int k = 1; k &LT;= 4; k++)\n                        for (int l = 1; l &LT;= 4; l++)\n                            for (int m = 1; m &LT;= 4; m++)\n                                for (int n = 1; n &LT;= 4; n++)\n                                {\n                                    str += NumberToChar(i);\n                                    str += NumberToChar(j);\n                                    str += NumberToChar(k);\n                                    str += NumberToChar(l);\n                                    str += NumberToChar(m);\n                                    str += NumberToChar(n);\n\n                                    array.Add(str);\n                                    str = \"\";\n                                }\n            return array;\n        }\n\n        public char NumberToChar(int n)\n        {\n            switch (n)\n            {\n                case 1:\/\/plus\n                    return '+';\n                case 2:\/\/Minus\n                    return '-';\n                case 3:\/\/Mutiply\n                    return '*';\n                case 4:\/\/Divide\n                    return '\/';\n                default: return ' ';\n            }\n        }\n        #endregion","answer_submit":1,"evaluation":300,"result":"5513936.58252427","lang":"C#","PHPDBID":56,"answerTimes":1},{"user_name":"\u0631\u0641\u064a\u0642 \u0627\u0644\u0627\u0645\u0644","algorithm_id":6,"start_time":1381083305,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":57,"answerTimes":0},{"user_name":"ahmedsaoud31","algorithm_id":6,"start_time":1381102347,"end_time":1381106415,"answer":"&LT;?php\n$arr = [8541,845,7415,415,15,852,85145];\n$arr2 = [8961,412,665,4215,412,62,9514];\n$op = ['+','-','*','\/'];\n$result = '46375267';\nfor(\/=0;\/&LT;4;++\/)\n{\n\tfor($j=0;$j&LT;4;++$j)\n\t{\n\t\tfor($k=0;$k&LT;4;++$k)\n\t\t{\n\t\t\tfor($l=0;$l&LT;4;++$l)\n\t\t\t{\n\t\t\t\tfor($m=0;$m&LT;4;++$m)\n\t\t\t\t{\n\t\t\t\t\tfor($n=0;$n&LT;4;++$n)\n\t\t\t\t\t{\n\t\t\t\t\t\tob_start();\n\t\t\t\t\t\t$var = \"(((((({$arr[0]}{$op[\/]}{$arr[1]}){$op[$j]}{$arr[2]}){$op[$k]}{$arr[3]}){$op[$l]}{$arr[4]}){$op[$m]}{$arr[5]}){$op[$n]}{$arr[6]})\";\n\t\t\t\t\t\teval(\"echo $var;\");\n\t\t\t\t\t\t$out = ob_get_contents();\n\t\t\t\t\t\tob_end_clean();\n\t\t\t\t\t\tif($out == $result)\n\t\t\t\t\t\t{\n\t\t\t\t\t\t\t$var .= ' = '.$result;\n\t\t\t\t\t\t\techo $var.'&LT;br&GT;';\n\t\t\t\t\t\t\t$op = [$op[\/],$op[$j],$op[$k],$op[$l],$op[$m],$op[$n]];\n\t\t\t\t\t\t\techo implode(' ',$op),'&LT;br&GT;';\n\t\t\t\t\t\t\t$var = \"(((((({$arr2[0]}{$op[0]}{$arr2[1]}){$op[1]}{$arr2[2]}){$op[2]}{$arr2[3]}){$op[3]}{$arr2[4]}){$op[4]}{$arr2[5]}){$op[5]}{$arr2[6]})\";\n\t\t\t\t\t\t\techo $var.' = ';\n\t\t\t\t\t\t\teval(\"echo $var;\");\n\t\t\t\t\t\t}\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n}\n\/\/ + - * \/ * -\n\/\/ ((((((8541+845)-7415)*415)\/15)*852)-85145) = 46375267\n\/\/ ((((((8961+412)-665)*4215)\/412)*62)-9514) = 5513936.5825243\n?&GT;","answer_submit":1,"evaluation":-1,"result":"5513936.5825243","lang":"PHP","PHPDBID":58,"answerTimes":1},{"user_name":"khatibe_30","algorithm_id":6,"start_time":1381222595,"end_time":1381225747,"answer":"\/\/solution: 5513936,5825243\n\/\/Language: C\n#include &LT;stdio.h&GT;\n#include &LT;stdlib.h&GT;\ndouble nbrs[] = {845, 7415, 415, 15, 852, 85145};\ndouble nbrs2[] = {412, 665, 4215, 412, 62, 9514};\nchar ops[6] = {0};\nvoid calc(int level, double result, int * gotit)\n{\n\tdouble res = 0;\n\tif(level == 6){\n\t\tif(result == 46375267)\n\t\t\t*gotit = 1;\n\t\treturn;\n\t}\n\tops[level] = '+';\n\tres = result + nbrs[level];\n\tcalc(level +1, res, gotit);\n\tif(*gotit) return;\n\tops[level] = '-';\n\tres = result - nbrs[level];\n\tcalc(level +1, res, gotit);\n\tif(*gotit) return;\n\tops[level] = '*';\n\tres = result * nbrs[level];\n\tcalc(level +1, res, gotit);\n\tif(*gotit) return;\n\tops[level] = '\/';\n\tres = result \/ nbrs[level];\n\tcalc(level +1, res, gotit);\n\tif(*gotit) return;\n}\nvoid main() {\n\tint gotit = 0;\n\tdouble result = 8541;\n\tcalc(0, result, &gotit);\n\tresult = 8961;\n\tfor(gotit = 0; gotit &LT; 6; gotit++){\n\t\tswitch(ops[gotit]){\n\t\t\tcase '+':result += nbrs2[gotit];break;\n\t\t\tcase '-':result -= nbrs2[gotit];break;\n\t\t\tcase '*':result *= nbrs2[gotit];break;\n\t\t\tcase '\/':result \/= nbrs2[gotit];break;\n\t\t}\n\t}\n\tprintf(\"%.7f\\n\", result);\n\tsystem(\"pause\");\n}","answer_submit":1,"evaluation":300,"result":"5513936,5825243","lang":"C","PHPDBID":59,"answerTimes":1},{"user_name":"samir ibrahim","algorithm_id":6,"start_time":1381304106,"end_time":1381317104,"answer":"Private Sub btnButton1_Click( sender As Object,  e As EventArgs) Handles btnButton1.Click\n        Dim dt As New DataTable\n        Dim op As String() = {\" \",\"+\",\"-\",\"\/\",\"*\"}\n        Dim s1,s2,s3,s4,s5,s6 As Double\n        Dim r1,r2,r3,r4,r5,r6 as Double\n        Dim result As Double = 0\n        Dim n1,n2,n3,n4,n5,n6,n7 As Double\n        n1 = 8541.00 : n2 = 845.00 : n3 = 7415.00 : n4 = 415.00 : n5 = 15.00 : n6 = 852.00 : n7 = 85145.00\n        For i = 1 to 4\n            s1 = i\n            r1 = Compute(n1,op(i),n2)\n            For j = 1 to 4\n                s2 = j\n                r2 = Compute(r1,op(j),n3)\n                For k = 1 to 4\n                    s3 = k\n                    r3 = Compute(r2,op(k),n4)\n                    For l = 1 to 4\n                        s4 = l\n                        r4 = Compute(r3,op(l),n5)\n                        For m = 1 to 4\n                            s5 = m\n                            r5 = Compute(r4,op(m),n6)\n                            For n = 1 to 4\n                                s6 = n\n                                r6 = Compute(r5,op(n),n7)\n                                result = r6\n                                Try\n                                    If CInt(result) = 46375267\n                                        MsgBox(op(s1) + \" \" + op(s2) + \" \" + op(s3) + \" \" + op(s4) + \" \" + op(s5) + \" \" + op(s6))\n                                    End If\n                                Catch ex As Exception\n\n                                End Try\n                            Next\n                        Next\n                    Next\n                Next\n            Next\n        Next\nEnd Sub\n    Function compute(ByVal n1,op,n2)\n        If op = \"+\"\n            Return n1 + n2\n        End If\n        If op = \"-\" \n            Return n1-n2\n        End If\n        If op = \"*\"\n            Return n1*n2\n        End If\n        If op = \"\/\"\n            Return n1\/n2\n        End If\n    End Function","answer_submit":1,"evaluation":300,"result":"5513936.5825242","lang":"VB.NET","PHPDBID":60,"answerTimes":1},{"user_name":"fmgret12","algorithm_id":6,"start_time":1381375819,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":61,"answerTimes":0},{"user_name":"benabdellah","algorithm_id":6,"start_time":1381482780,"end_time":0,"answer":"","answer_submit":false,"evaluation":-1,"result":"none","lang":"none","PHPDBID":62,"answerTimes":0},{"user_name":"SharpSword","algorithm_id":7,"start_time":1381536386,"end_time":1381579549,"answer":"using System;\nusing System.Collections.Generic;\nusing System.Text;\n\nnamespace ConsoleApplication2\n{\n    class Program\n    {\n        static void Main(string[] args)\n        {\n            int demontionMatrix = 12;\n            int sommeKilometre = 0;\n            int longCarre = 550;\n            for (int i = 1; i &LT; demontionMatrix ; i++)\n            {\n                sommeKilometre = sommeKilometre + (i * i * 4 * longCarre);\n                longCarre = longCarre - 50;\n            }\n            Console.Write(\"The price is \");\n            Console.Write(sommeKilometre * 5);\n            Console.Write(\" Liver\");\n            \n            \/\/ Exit Part \n            Console.ReadKey();\n        }\n    }\n}\n","answer_submit":1,"evaluation":300,"result":"1716000","lang":"C#","PHPDBID":63,"answerTimes":1},{"user_